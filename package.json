{
  "name": "subtitles-app",
  "version": "0.6.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "verify": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "generate-schema": "source .env && dotenv -c -- bash -c \"gql.tada generate schema https://graphql.datocms.com --header 'X-Exclude-Invalid: true' --header 'Authorization: $DATOCMS_PUBLISHED_CONTENT_CDA_TOKEN'\""
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.7",
    "@datocms/cma-client": "^3.4.5",
    "@hookform/resolvers": "^3.10.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@upstash/ratelimit": "^2.0.1",
    "@vercel/analytics": "^1.3.1",
    "@vercel/functions": "^2.2.12",
    "@vercel/kv": "^2.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.5.1",
    "datocms-structured-text-to-plain-text": "^4.0.1",
    "dotenv": "^16.5.0",
    "gql.tada": "^1.8.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "next": "^15.5.2",
    "next-auth": "^4.24.11",
    "next-intl": "^3.26.5",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-datocms": "^7.2.2",
    "react-dom": "19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^10.0.6",
    "serialize-error": "^11.0.3",
    "sharp": "^0.33.5",
    "uuidv4": "^6.2.13",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@types/react-google-recaptcha": "^2.1.9",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
